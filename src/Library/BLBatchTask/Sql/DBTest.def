#Key   = TEST_DATE`>=`CONVERT(varchar(8), @KEY, 112)`ToString("YYYYmmDD")
#Oracle
#Key   = TEST_DATE`>=`TO_CHAR(@KEY, 'YYYYMMDD')`ToString("YYYYmmDD")

Table DBTest
Y Y TestId           INT      4    0
N N TestValue        VARCHAR  MAX  ""
N N TestDate         DATETIME 8    Now()
End Table

Query ByTestValue
Key   = TestValue
Order = TestId`DESC
End Query

Query ByTestDate
Key   = TestDate`>=`AND`TODATETIME
End Query

Query ByTestValue1
Key   = TestValue`LIKE
End Query

Count ByTestValue
Key   = TestValue
End Count