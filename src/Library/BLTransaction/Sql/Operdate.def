Table Operdate
N N OP_DATE        VARCHAR 8    0
N N DATE_FLAG      VARCHAR 2    ""
N N IS_OP_DATE     VARCHAR 2    ""
End Table

Query ByOperDate
Key=OP_DATE,IS_OP_DATE
Order = OP_DATE`ASC
End Query

Query NextDateByOperDate
Key = OP_DATE`>,IS_OP_DATE
Order = OP_DATE`ASC
End Query

Query PreDateByOperDate
Key = OP_DATE`<,IS_OP_DATE
Order = OP_DATE`DESC
End Query