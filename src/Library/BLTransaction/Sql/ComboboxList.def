Table ComboboxList
N N ComboId            VARCHAR  4    0
N N ComboValue         VARCHAR  MAX  ''
N N ComboDescription   VARCHAR  MAX  ''
N N Priority           INT      4    0
End Table

Query ByComboId
Key=ComboId
Order = Priority`ASC
End Query

Query ByTELECODE
Key=ComboId,ComboValue
End Query

Query LikeComboId
Key=ComboId`LIKE
Order = Priority`ASC
End Query